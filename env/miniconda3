# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/pmitev/miniconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/pmitev/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/home/pmitev/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/pmitev/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

#conda install argcomplete
eval "$(register-python-argcomplete conda)"
# export SETUPTOOLS_USE_DISTUTILS=stdlib


#If you'd prefer that conda's base environment not be activated on startup, 
#   set the auto_activate_base parameter to false: 
#
#conda config --set auto_activate_base false
#
